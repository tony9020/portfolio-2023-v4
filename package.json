{
  "private": true,
  "name": "tonychristopherofficial.com",
  "version": "0.0.0",
  "description": "My personal website and blog built with TypeScript, Next.js, and Tailwind CSS",
  "license": "AGPL-3.0-only",
  "author": "tonychristopherofficial <me@tonychristopherofficial.com> (https://github.com/tonychristopherofficial/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonychristopherofficial/tonychristopherofficial.com.git"
  },
  "type": "module",
  "scripts": {
    "build": "next build",
    "check-updates": "npx npm-check-updates --interactive --format group",
    "clean": "rm -rf .next .contentlayer coverage playwright-report test-results tsconfig.tsbuildinfo",
    "dev": "concurrently \"contentlayer dev\" \"cross-env NODE_OPTIONS='--inspect' next dev\"",
    "format": "prettier -w .",
    "preinstall": "npx only-allow yarn",
    "postinstall": "prisma generate && contentlayer build",
    "lint": "next lint",
    "lint:fix": "eslint --fix .",
    "prepare": "husky install",
    "start": "next start",
    "test:coverage": "vitest --coverage --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:unit": "vitest run",
    "test:unit:ui": "vitest --ui",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@giscus/react": "^2.3.0",
    "@octokit/rest": "^20.0.2",
    "@prisma/client": "^5.6.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tabler/icons-react": "^2.41.0",
    "@types/hast": "^3.0.3",
    "canvas-confetti": "^1.9.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "contentlayer": "^0.3.4",
    "dayjs": "^1.11.10",
    "framer-motion": "^10.16.5",
    "github-slugger": "^2.0.0",
    "hastscript": "^8.0.0",
    "marked": "^10.0.0",
    "next": "^14.0.3",
    "next-auth": "^5.0.0-beta.3",
    "next-contentlayer": "^0.3.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-medium-image-zoom": "^5.1.8",
    "react-use": "^17.4.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.10.2",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "3.0.1",
    "rss": "^1.2.2",
    "sharp": "^0.32.6",
    "shiki": "^0.14.5",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "unist-util-visit": "^5.0.0",
    "use-debounce": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@octokit/types": "^12.3.0",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@tonychristopherofficial/eslint-config": "1.0.35",
    "@tonychristopherofficial/prettier-config": "^1.0.8",
    "@tonychristopherofficial/tsconfig": "^1.0.3",
    "@types/canvas-confetti": "^1.6.4",
    "@types/mdx": "^2.0.10",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/rss": "^0.0.32",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-plugin-playwright": "^0.18.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "fast-xml-parser": "^4.3.2",
    "husky": "^8.0.3",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.1.0",
    "msw": "^2.0.8",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "schema-dts": "^1.1.2",
    "tailwindcss": "^3.3.5",
    "typescript": "5.3.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">= 18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "next/core-web-vitals",
      "@tonychristopherofficial/eslint-config"
    ],
    "parserOptions": {
      "project": "./tsconfig.json",
      "ecmaVersion": "latest"
    },
    "overrides": [
      {
        "files": [
          "./src/tests/!(e2e)/**/*.test.{ts,tsx}"
        ],
        "extends": [
          "plugin:testing-library/react"
        ],
        "plugins": [
          "testing-library"
        ]
      },
      {
        "files": [
          "./src/tests/e2e/**/*.test.{ts,tsx}"
        ],
        "extends": [
          "plugin:playwright/recommended"
        ]
      }
    ]
  },
  "lint-staged": {
    "*.{js,jsx,cjs,mjs,ts,tsx}": [
      "eslint"
    ],
    "*.{json,css,md,mdx,js,jsx,ts,tsx,cjs,mjs}": [
      "prettier -c"
    ]
  },
  "packageManager": "yarn@4.0.0",
  "prettier": "@tonychristopherofficial/prettier-config"
}
